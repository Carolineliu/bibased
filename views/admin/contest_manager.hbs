{{!< default}}
{{#contentFor 'adminPageStyles'}}
  <link rel="stylesheet" href="../css/admin/contest_manager.css">
  <script type="text/javascript" src="../jquery/jquery.pagination.js"></script>
  <script type="text/javascript" src="../js/admin/contest_manager.js"></script>
{{/contentFor}}

<div class="container">
  <div class="contestsTypeName">
    <h3>作品分类</h3>
  </div>

  <div class="participatingContent" id="participatingContent">
    <div class="participatingSchool">
      <span class="participatingSchoolName">学校：</span>
      <ul class="schools">
        <li class="contestsType open" id="schools"><a href="#">全部</a></li>
        {{#each schools}}
          <li class="contestsType" id="schoolSingle"><a href="#">{{schoolName}}</a></li>
        {{/each}}
      </ul>
    </div>

    <div class="participatingYear" id="participatingYear">
      <span class="participatingTypeName">年份：</span>
      <select class="years">
        {{#each year}}
          <option class="contestsYear">{{this}}</option>
        {{/each}}
      </select>
    </div>
  </div>

  <table class="contestContent">
    <thead id="contestContents">
      <tr>
        <th data-name="contestId">项目编号</th>
        <th data-name="contestName">项目名称</th>
        <th data-name="leader">项目负责人</th>
        <th data-name="studentCount">其他成员</th>
        <th data-name="tutor">指导老师</th>
        <th data-name="year">作品日期</th>
        <th data-name="school">学校</th>
        <th data-name="prelim">初赛成绩</th>
        <th data-name="final">复赛成绩</th>
        <th data-name="isWin">是否获奖</th>
        <th>操作</th>
      </tr>
    </thead>
    <tbody>

    </tbody>
  </table>
  <div id="page" class="pagination" data-name= {{length}}></div>
  <div class="noResult">此次查询没有结果</div>
</div>
